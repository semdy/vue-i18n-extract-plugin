{
  "name": "extract-i18n-plugin",
  "version": "1.1.9",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "bin": {
    "extract-i18n": "lib/cli.js"
  },
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.js",
      "types": "./types/index.d.ts"
    },
    "./translators": {
      "require": "./lib/translators/index.js",
      "import": "./lib/translators/index.js",
      "types": "./types/translators/index.d.ts"
    },
    "./babel-plugin-i18n": {
      "require": "./lib/babel-plugin-i18n.js",
      "import": "./lib/babel-plugin-i18n.js",
      "types": "./types/babel-plugin-i18n.d.ts"
    },
    "./babel-plugin-i18n-import": {
      "require": "./lib/babel-plugin-i18n-import.js",
      "import": "./lib/babel-plugin-i18n-import.js",
      "types": "./types/babel-plugin-i18n-import.d.ts"
    },
    "./webpack-i18n-loader": {
      "require": "./lib/webpack-i18n-loader.js",
      "import": "./lib/webpack-i18n-loader.js",
      "types": "./types/webpack-i18n-loader.d.ts"
    },
    "./webpack-i18n-import-loader": {
      "require": "./lib/webpack-i18n-import-loader.js",
      "import": "./lib/webpack-i18n-import-loader.js",
      "types": "./types/webpack-i18n-import-loader.d.ts"
    },
    "./client": {
      "types": "./types/extract-i18n.config.d.ts"
    }
  },
  "keywords": [
    "i18n-extract-plugin",
    "i18n",
    "i18n-extract",
    "vue-i18n",
    "react-intl",
    "react-i18next",
    "react-i18n"
  ],
  "files": [
    "lib",
    "types",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "author": "501859275@qq.com",
  "homepage": "https://github.com/semdy/extract-i18n-plugin",
  "license": "MIT",
  "description": "A helpfull plugin for extracting i18n strings from your code and translate/transform them.",
  "dependencies": {
    "@babel/core": "^7.27.3",
    "@babel/generator": "^7.27.3",
    "@babel/helper-plugin-utils": "^7.27.1",
    "@babel/parser": "^7.27.3",
    "@babel/traverse": "^7.27.3",
    "@babel/types": "^7.27.3",
    "@rollup/pluginutils": "^5.1.4",
    "@types/tunnel": "^0.0.7",
    "@vitalets/google-translate-api": "^9.2.0",
    "@vue/compiler-dom": "^3.5.16",
    "@vue/compiler-sfc": "^3.5.16",
    "axios": "^1.6.8",
    "c12": "^3.0.4",
    "crypto-js": "^4.2.0",
    "esbuild": "^0.27.2",
    "esbuild-register": "^3.6.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "magic-string": "^0.30.17",
    "prettier": "^3.5.3",
    "tunnel": "0.0.6"
  }
}
